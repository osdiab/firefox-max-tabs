{
  "manifest_version": 2,
  "name": "Tab Limit",
  "version": "0.0.1",

  "description": "Set a limit for the number of opened tabs",

  "icons": {
    "48": "icons/48.svg"
  },

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["__build/content.js"]
    }
  ],
  "background": {
    "scripts": ["__build/background.js"]
  },
  "options_ui": {
    "page": "src/options_ui/index.html"
  },

  "permissions": [
    "tabs"
  ]

}
